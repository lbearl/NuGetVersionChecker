@model IEnumerable<NuGetLib.NuGetData>
@{
    ViewBag.Title = "Single Result";
}
<h2>@ViewBag.Title.</h2>

<table class="table table-hover">
    <thead>
    <tr>
        <th>Package Id</th>
        <th>Package Published On</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var x in @Model)
    {
        <tr>
            <td>@x.PackageId</td>
            <td>@x.PublishedDate</td>
        </tr>
    }
    </tbody>
</table>
