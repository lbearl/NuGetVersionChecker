@model Dictionary<string, IEnumerable<NuGetLib.NuGetData>>
@{
    ViewBag.Title = "Results";
}

<h2>View</h2>

@foreach (var d in Model)
{
    <h1>@d.Key</h1>
    <table class="table table-hover">
        <thead>
        <tr>
            <th>Package Id</th>
            <th>Package Published On</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var x in d.Value)
        {
            <tr>
                <td>@x.PackageId</td>
                <td>@x.PublishedDate</td>
            </tr>
        }
        </tbody>
    </table>
}